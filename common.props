<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Version>0.290.1</Version>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>EasyAbp Team</Authors>
    <Description>An abp module that provides standard tree structure entity implement.</Description>
    <PackageProjectUrl>https://github.com/EasyAbp/PrivateMessaging</PackageProjectUrl>
    <RepositoryUrl>https://github.com/EasyAbp/PrivateMessaging</RepositoryUrl>
    <PackageTags>abp module easyabp tree</PackageTags>
    <Company>EasyAbp</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.3" PrivateAssets="All" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ConfigureAwait.Fody" Version="3.3.1" />
    <PackageReference Include="Fody" Version="6.2.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>